    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Linux/x86_64/binderoo_util",
            "args": [
                "-f",
                "../binderooclienttest",
                "-gA"
            ],
            "cwd": "${workspaceRoot}",
            "sourceMap": {
                "/root/project/runtime/" : "/usr/lib/ldc2/src/runtime/"
            }
        }
    ]
}
