{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Linux/x86_64/binderoo_util",
            "args": [
                "-f",
                "../binderooclienttest",
                "-c",
                "binderoo.binding.cheaderparsing.ParseAndGenerateBinderooCodeFromHeader",
                "-p",
                "/usr/lib/x86_64-linux-gnu/wx/include/gtk2-unicode-3.0,/usr/include/wx-3.0",
                "-p",
                "wx/defs.h",
                "-p",
                "_FILE_OFFSET_BITS=64,WXUSINGDLL,__WXGTK__"
            ],
            "cwd": "${workspaceRoot}",
            "sourceMap": {
                "/root/project/runtime/" : "/usr/lib/ldc2/src/runtime/"
            }
        }
    ]
}
